{
  "expo": {
    "name": "DoneWithIt",
    "slug": "DoneWithIt",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e63c4b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.startapp.donewithit",
      "buildNumber": "1.0.0"
    },
    
    "android": {
      "package": "com.startapp.donewithit"
    },
    "description": "A marketplace for selling the stuff you don't want",
    "notification": {
      "iosDisplayInForeground": true
    },

    "extra": {
      "bugsnag": {
        "apiKey": "789aec7212c7b5f29ef3ee651e94939f"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "@bugsnag/expo/hooks/post-publish.js",
          "config": {}
        }
      ]
    }
  }
}